variable "db_user" {
  description = "The username of the RDS instance"
  type        = string
  default     = "postgres"
}

variable "db_name" {
  description = "The password of the RDS instance"
  type        = string
  default     = "postgres"
}

variable "db_password" {
  description = "The password of the RDS instance"
  type        = string
  default     = "postgres"
}
